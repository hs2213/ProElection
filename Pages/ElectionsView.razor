@using ProElection.Entities
@inherits ProElection.Shared.ComponentBases.CheckAuthentication
@page "/elections"
<h1>Elections</h1>

@if (ViewingUser == null)
{
    <p>User Not Found...</p>
}
else
{
    <div class="election-list">
        @foreach (Election election in _elections)
        {
            <SingleElection
                Election="election"
                ViewingUser="ViewingUser"/>        }
    </div>
}
